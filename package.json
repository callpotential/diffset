{
  "name": "diffset",
  "version": "0.1.5",
  "private": true,
  "description": "GitHub Action for producing list of files that changed between branches",
  "main": "lib/main.js",
  "scripts": {
    "build": "ncc build src/main.ts --minify",
    "buildcheck": "tsc --noEmit -p .",
    "test": "jest --coverage",
    "fmt": "prettier --write 'src/**/*.ts' '__tests__/**/*.ts'",
    "fmtcheck": "prettier --check 'src/**/*.ts' '__tests__/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/softprops/diffset.git"
  },
  "keywords": [
    "actions"
  ],
  "author": "softprops",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.3",
    "@octokit/plugin-throttling": "^3.4.1",
    "@octokit/rest": "^18.3.5",
    "minimatch": "^3.0.4"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/minimatch": "^3.0.3",
    "@types/node": "^12.20.5",
    "@vercel/ncc": "^0.27.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "ts-jest": "^26.5.3",
    "typescript": "^3.9.9"
  },
  "lint-staged": {
    "{__tests__,src}/**/*.ts": [
      "npm run fmt",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
